@model List<CompetencyMatrix.ViewModels.PositionMatrixDetails>

<style>
	.breadcrumb > li + li:before {
		color: #808080;
		content: "> ";
	}
</style>

<ol class="breadcrumb" style="margin: 0px; padding: 0px">
	@for (int i = 0; i < Model.Count; i++)
	{
		<li>
			<a href="javascript:selectPositionMatrix(@Model[i].Id)">@Model[i].Name</a>
		</li>
	}
</ol>
